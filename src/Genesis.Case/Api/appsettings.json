{
  "Kestrel": {
    "EndPoints": {
      "Http": {
        "Url": "http://*:80"
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Email": {
    "Login": "${SENDER_EMAIL_LOGIN}",
    "Password": "${SENDER_EMAIL_PASSWORD}"
    "SmtpHost": "smtp.gmail.com",
    "Port": 587,
    "SenderName": "Fusion Exchange Rate Tracker"
  },
  "FileStorageConfig": {
    "Filename": "emails.json",
    "IORetryCount": 5,
    "IORetryDelayMilliseconds": 300
  },
  
  "AllowedHosts": "*"
}
